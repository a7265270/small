.cover-image{
  width:48rpx;
  height:48rpx;
  position: absolute;
  left: 50%;
  top:50%;
  margin-left: -24rpx;
  margin-top:-58rpx;
}
.no-store{font-size:28rpx; padding: 20rpx;}

.section{position:absolute; left:24rpx; right:24rpx; z-index:99; top:24rpx;}
.section input{background:#fff; height:64rpx; font-size:28rpx; padding:0 20rpx; border:0; border-radius:6rpx; box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.1);}
.text_box{margin:0 24rpx; border-bottom:0rpx solid #eee; padding:16rpx 0; font-size:26rpx;}
.store-map{height:calc(100% - 250rpx);}
.map{width:100%; height:100%;}
.store-map-wrap{background:#fff; position:fixed; width:100%;  left:0; z-index:99; bottom:0;height:250rpx;}
.mapBtn{padding:24rpx;}
.mapBtn button{background:#fe4f4f; color:#fff; width:100%; margin:0; height:80rpx; line-height:60rpx; font-size:28rpx; border:0; border-radius:10rpx;}
.store-detail-address-box{font-size:28rpx;}
.mapSearchBox{background:#fff; max-height:600rpx; overflow-y:auto; padding:10rpx 0; border-radius:10rpx; position:absolute; top:100rpx; left:24rpx; right:24rpx; z-index:99; box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.1);}
.getLocation{position:absolute; z-index:99; width:80rpx; height:80rpx; text-align:center; line-height:80rpx; border-radius:10rpx; right:24rpx; bottom:280rpx; background:#fff; box-shadow:0 4rpx 16rpx rgba(0, 0, 0, 0.1);}
.cover-image2{width:48rpx; height:48rpx; margin:16rpx auto;}
.getLocationActive{background:#f7f7f7;  box-shadow:0 4rpx 16rpx rgba(0, 0, 0, 0.2);}


.search-container{padding-top:106rpx;}
.searchHeader{ height:90rpx; background:#fff;  line-height:90rpx; position:fixed; z-index:99;  top:0; left:0; padding:0 24rpx; box-sizing:border-box; display:flex; align-items:center; }
.searchHeader .headerSearchInput{position:relative; width:calc(100% - 100rpx); margin-left:20rpx;}
.search-input{border:0; font-size:28rpx; padding:0 24rpx; height:64rpx; background:#f2f2f2; border-radius:40rpx;}
.search-delete{position:absolute; right:16rpx; background:#bbb; top:16rpx; width:38rpx; height:38rpx; text-align:center; line-height:32rpx; z-index:2; border-radius:100%; color:#fff; font-size:32rpx;}
.searchHeaderCanle{font-size:26rpx; color:#666;}
.search-keyword-list{padding:24rpx; margin-top:0rpx; background-color:#fff; overflow: hidden; position:relative;}
.search-taps{overflow:hidden;}
.search-taps-li{float:left; line-height:52rpx; overflow:hidden; margin:16rpx 20rpx 0rpx 0px; font-size:26rpx;  background:#f7f7f7; border-radius:50rpx;  padding:0 24rpx; color:#666;}
.search-taps-li navigator{ display: block;} 
.search-title{overflow:hidden; line-height:48rpx; font-size:28rpx; font-weight:bold; color:#333; display:flex; align-items:center; justify-content:space-between;}
.searchRecordDelete{font-size:38rpx; color:#999; font-weight:normal;}