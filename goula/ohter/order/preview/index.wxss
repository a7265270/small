@import "../../../../icon.wxss";
/*订单结算*/
.orderDetailAddress{background:#fff url(https://cdn.momsurprise.com/static/img/address-bg.png) no-repeat bottom; background-repeat:repeat-x; background-size:40% auto; position:relative; margin-bottom:20rpx; border-radius:0 0  10rpx 10rpx; padding:24rpx 48rpx 28rpx 24rpx; overflow:hidden; display:flex; }
.orderAddressIcon{font-size:40rpx; color:#666;}
.addressUser{font-size:28rpx; display:flex; }
.addressUser text{margin-right:100rpx;}
.orderAddress{width:calc(100% - 60rpx); margin-left:10rpx;}
.orderAddressR{font-size:28rpx; color:#999; position:absolute; right:24rpx; top:50%; margin-top:-28rpx;}
.receivingAddress{font-size:24rpx; padding-top:10rpx;}
.addAddress{width:100%; padding:10rpx 0; display:flex; align-items:center; justify-content:start; font-size:26rpx; color:#333;}
.addAddress label{text-align:center; margin-right:10rpx; font-size:32rpx; color:#fff; text-align:center; line-height:48rpx; width:48rpx; height:48rpx; border-radius:6rpx; background:#3283f9;}
.orderGoodList{background:#fff; margin:20rpx 0; border-radius:10rpx;}
.goodGroup{display:flex; justify-content:space-between; align-items:center; padding:24rpx; border-top:1rpx #ddd solid;}
.goodGroup:first-child{border:0;}
.goodGroupImg{width:180rpx; height:180rpx; border-radius:10rpx; overflow:hidden;}
.goodGroupImg image{width:180rpx; height:180rpx; display:block;}
.goodGroupRh{width:calc(100% - 200rpx); margin-left:20rpx; height:180rpx; position:relative;}
.shopcartNumber{display:flex; align-items:center; font-size:24rpx; color:#999;}
.shopcartNumber input{width:80rpx; height:52rpx; color:#333; text-align:center; font-size:28rpx;}
.shopcartNumber button{background:#ccc; margin:0; color:#fff; width:52rpx; height:52rpx; padding:0px; text-align:center; line-height:52rpx; font-size:28rpx; border-radius:4rpx; border:0rpx;}
.goodGroupRh .h4{font-size:28rpx; height:80rpx;}
.goodGroupRh .p{font-size:24rpx; color:#999;}
.goodGroupRh .p text{padding-right:48rpx;}
.goodsettlementBot{display:flex; align-items:flex-end; justify-content:space-between; position:absolute; bottom:0; left:0; right:0;}
.goodGroupPrice{color:#fe4f4f; font-size:28rpx;}
.blanceMoney{background:#fff; margin:20rpx 0; border-radius:10rpx; padding:10rpx 24rpx;}
.blanceMoneyItem{display:flex; justify-content:space-between; line-height:70rpx; font-size:26rpx;}
.blanceMoneyItem view text{color:#fe4f4f;}
.blanceMoneyItem view{color:#fe4f4f;}
.blanceMoneyItem view.switch{display:flex; align-items:center;}
.blanceMoneyItem view.switch switch{margin-left:20rpx;}
.blanceInfo{background:#fff; padding:20rpx 24rpx; border-radius:10rpx; overflow:hidden;}
.blanceInfo{overflow:hidden; line-height:64rpx;}
.blanceInfo text{float:left; font-size:26rpx; width:150rpx;}
.blanceInfo input{float:left; font-size:26rpx; height:64rpx; width:calc(100% - 150rpx); border:0;}
.blanceFooter{position:fixed;box-shadow:0 0 20rpx rgba(0, 0, 0, 0.05); z-index:99; padding:0rpx 0 0 24rpx; display:flex; align-items:center; justify-content:space-between; bottom:0; background:#fff; left:0; right:0;}
.blanceMoney{background:#fff; padding:20rpx 24rpx; font-size:26rpx; overflow:hidden; margin:20rpx 0; border-radius:10rpx;}
.shopcartBlance button{background:linear-gradient(to right, #f86551, #d81e06); color:#fff; font-size:28rpx; border:0; line-height:68rpx; height:88rpx; width:240rpx; border-radius:0rpx;}
.shopcartMoney{font-size:26rpx;}
.shopcartMoney text{color:#fe4f4f; font-size:32rpx; font-weight:bold;}
.radio{transform:scale(0.8);}
.dialogDetailBtn{overflow:hidden; padding:24rpx 150rpx;}
.dialogDetailBtn button{color:#fff; border-radius:40rpx; background:linear-gradient(to right, #eb8a7e, #d81e06);   font-size:28rpx; border:0; width:100%; height:72rpx; line-height:72rpx; margin:0;}
.goodDetail{
  padding-bottom: 100rpx;
}

/**优惠券*/

.couponGetDialog{position:fixed; left:0; top:0; bottom:0; right:0; z-index:999; opacity:0; pointer-events:none;}
.couponGetDialog.show{opacity:1.0; pointer-events:auto;}
.couponGetDialog.show .mask{pointer-events:auto;}
.couponGetDialog.show .couponGet{opacity: 1; bottom:0; transition-duration: 0.3s; -ms-transform: scale(1); transform: scale(1); pointer-events: auto;}
.couponGet{position:fixed; z-index:999; left:0; bottom:-750rpx; right:0; background:#f9f9f9; -ms-transform: scale(1.185); transform: scale(1.185); border-radius:10rpx 10rpx 0 0; overflow:hidden; backface-visibility: hidden; pointer-events: none; transition: all 0.3s ease-in-out 0s;}
.couponGetTitle{position:relative; padding-left:24rpx; font-size:32rpx; line-height:80rpx; background:#fff;}
.couponGetTitle icon{position:absolute; right:24rpx; top:0; font-size:38rpx; color:#999;}
.getCouponTab{background:#fff; height:88rpx; line-height:80rpx; display:flex;}
.getCouponTabItem{flex:1; position:relative; text-align:center; font-size:26rpx; color:#666;}
.getCouponTabItem.active{color:#000; font-weight:bold;}
.getCouponTabItem.active::after{content:""; position:absolute; height:4rpx; background:linear-gradient(to right, #fff,#b622ff); width:56rpx; left:50%; margin-left:-28rpx; bottom:16rpx;}
.couponGetList{height:750rpx; overflow:auto; -webkit-overflow-scrolling:touch;}
.couponGetItem{background:#fff; display:flex; flex-wrap: wrap; border-radius:10rpx; overflow:hidden; box-shadow:0 0 20rpx rgba(0,0,0,0.04); margin:24rpx 24rpx 0rpx 24rpx;}
.tabs-container .couponGetItem{margin:4rpx 24rpx 24rpx 24rpx;}
.couponItemLf{background:-webkit-linear-gradient(top left, #21d4fd, #b721ff); border-right:1px #fff dashed; width:180rpx; padding:24rpx 0; color:#fff; text-align:center; position:relative;}
.couponItemLf:before{content:""; position:absolute; width:20rpx; height:20rpx; background:#fff; border-radius:100%; right:-10rpx; top:-10rpx;}
.couponItemLf:after{content:""; position:absolute; width:20rpx; height:20rpx; background:#fff; border-radius:100%; right:-10rpx; bottom:-10rpx;}
.couponItemLf .h4{display:flex; font-size:24rpx; justify-content:center; align-items:flex-end;}
.couponItemLf .h4 text{font-size:48rpx; position:relative; bottom:-4rpx;}
.couponItemLf .h6{font-size:24rpx; padding-top:14rpx;}
.couponItemRh{width:calc(100% - 180rpx); padding:20rpx; position:relative;}
.couponItemRh .h4{font-size:26rpx; font-weight:bold; height:80rpx;}
.couponItemRh .text{color:#999; font-size:22rpx; display:flex; justify-content:space-between; align-items:flex-end;}
.couponItemRh .text .radio{margin-right:-10rpx;}
.couponItemRh .text button{border:0; background:#fe4f4f; color:#fff; line-height:48rpx; border-radius:40rpx; font-size:24rpx; height:48rpx; padding:0 10rpx; width:120rpx; margin:0;}
.couponGetItem.invalid .couponItemLf{background:-webkit-linear-gradient(top left, #ddd, #bbb); color:#f2f2f2;}
.couponGetItem.invalid .couponItemRh{opacity:0.8;}
.couponGetItem.invalid .couponItemRh .h4{color:#999;}
.couponDisabledText{padding:18rpx 20rpx; color:#666; position:relative; z-index:9; font-size:24rpx; background:#fff; box-shadow:0 -2rpx 20rpx rgba(0,0,0,0.05); width:100%;}
.couponGq{position:absolute; right:-20rpx; top:-20rpx; z-index:9; opacity:0.5;}
.couponGq image{width:100rpx; display:block;}
.couponList .couponItemLf:before{content:""; position:absolute; width:20rpx; height:20rpx; background:#f8f8f8; border-radius:100%; right:-10rpx; top:-10rpx;}
.couponList .couponItemLf:after{content:""; position:absolute; width:20rpx; height:20rpx; background:#f8f8f8; border-radius:100%; right:-10rpx; bottom:-10rpx;}
