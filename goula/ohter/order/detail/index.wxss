@import "/icon.wxss";
/*订单详情*/
.expressMessageCompany{background:#fff; border-radius:10rpx 10rpx 0 0; line-height:48rpx; padding:20rpx 24rpx; font-size:28rpx; border-bottom:1rpx #ddd solid;}
.orderAddressed{background:#fff url(http://jingxi-pro.oss-cn-shenzhen.aliyuncs.com/static/v2/img/address-bg.png) no-repeat bottom; width:100%; background-size:50% auto; background-repeat:repeat-x;  margin-bottom:20rpx; overflow:hidden; padding:24rpx 24rpx 34rpx 24rpx; border-radius:10rpx;font-size: 24rpx;}
.orderAddressIcon{float:left; color:#999; font-size:42rpx;}
.orderAddressText{margin-left:60rpx; margin-right:24rpx;}
.receivingPers{font-size:30rpx;}
.receivingPers text{display:inline-block; vertical-align:middle; padding-right:38rpx;}
.orderAddressText .receivingAddress{padding-top:16rpx;}
.expressMessage{background:#fff; padding:24rpx; margin-bottom:20rpx; font-size:26rpx; border-radius:10rpx;}
.expressMessageTitle{font-size:28rpx; padding-bottom:24rpx;}
.expressItem{overflow:hidden; position:relative;}
.expressTime{float:left; color:#aaa; text-align:center; font-size:20rpx;}
.expressTime text{display:block;}
.expressTime text:nth-of-type(2){font-size:18rpx;}
.expressMessageText{margin-left:150rpx; padding:0rpx 0 38rpx 36rpx; color:#666; border-left:1rpx #ddd solid; position:relative;}
.expressItem:last-child .expressMessageText{border-left:0; padding-bottom:0;}
.expressLable{font-size:28rpx;  padding-bottom:4rpx; position:relative;}
.expressText{font-size:24rpx;}
.expressIcon{position:absolute; top:10rpx; left:150rpx; margin-left:-4rpx; width:12rpx; height:12rpx; background:#aaa; border-radius:100%; border:1rpx #fff solid; z-index:9;}
.expressIcon icon{ display:block;}
.bigIcon .expressIcon{width:38rpx; top:0; left:150rpx; margin-left:-19rpx; height:38rpx; background:#fff; border-radius:100%; border:1rpx #aaa solid;}
.bigIcon .expressIcon icon{display:block; width:18rpx; height:8rpx; border-left:2rpx #aaa solid; border-bottom:2rpx #aaa solid; transform:rotate(-45deg); margin:12rpx auto;}

.bigIconActive .expressIcon{width:38rpx; top:0; left:150rpx; margin-left:-19rpx; height:38rpx; background:#7b69fe; border-radius:100%; border:1rpx #fff solid; text-align:center; font-size:24rpx; line-height:38rpx; }
.bigIconActive .expressIcon icon{display:inline-block; font-size:24rpx; color:#fff; margin-top:-10rpx;}
.bigIconActive .expressMessageText{color:#000;}
.orderpay-info{background:#fff; padding:10rpx 0; margin-top:20rpx; font-size:24rpx; border-radius:10rpx;}
.record-info-p{overflow:hidden; line-height:64rpx; padding:0 24rpx;}
.record-info-p text{float:right; line-height:64rpx; font-size:24rpx;}
.record-info-p b{color:#fe4f4f;}
.record-info-p .text{float:right; text-align:right;}
.discount-amount{margin:20rpx auto; border-radius:10rpx; padding:38rpx 0; text-align:center; color:#fff; font-size:50rpx;
background:url(http://jingxi-pro.oss-cn-shenzhen.aliyuncs.com/static/v2/img/discount-amount-bg.png); width:90%; height:75rpx; background-size:100% 150rpx;}
.discount-amount text{font-size:38rpx; display:inline-block; vertical-align:middle;}
.discount-amount b{display:inline-block; vertical-align:middle;}
.record-info-p button{display:inline-block; padding:0 0 0 38rpx; color:#fe4f4f; margin-top:13rpx;font-size:24rpx; float:right; background:transparent;width:120rpx !important;}
.orderDetailState{background:#d81e06; color:#fff; line-height:64rpx; display:flex; align-items:center; justify-content:center; text-align:center; font-size:32rpx; padding:30rpx 0 32rpx 0;}
.orderDetailState label{font-size:38rpx; font-weight:bold; padding-right:10rpx;}
.orderDetailState label image{width:56rpx; height:56rpx; display:block;}
.orderDetailBtn{background:#fff; box-shadow:0 0 20rpx rgba(0, 0, 0, 0.1); position:fixed; bottom:0; left:0; right:0; text-align:right; padding:16rpx 24rpx;}
.orderDetailBtn button{display:inline-block; margin-left:20rpx; vertical-align:middle;width:200rpx !important;margin-left:10rpx !important;}
.orderDetail{padding-bottom: 200rpx;}

.tabs-container{padding-top:106rpx;}
.orderItem{margin-bottom:20rpx;}
.orderAll{background:#fff; border-radius:10rpx; overflow:hidden;}
.orderState{display:flex; color:#999; justify-content:space-between; padding:0 24rpx; font-size:24rpx; line-height:80rpx; border-bottom:1rpx #ddd solid;}
.orderState .order-list-state{color:#d81e06;}
.orderTime{font-size:24rpx; color:#999;}
.orderListNum{display:flex; align-items:baseline; justify-content: flex-end; height:72rpx; line-height:72rpx; text-align: right; font-size:37rpx;}
.orderListNum label{font-size:24rpx; padding-right:20rpx;}
.orderListNum text{font-size:30rpx;}
.orderListAll{display:flex; align-items:center; justify-content:space-between; padding:10rpx 24rpx 0 24rpx;}
.orderBottomBtn{padding:0 24rpx 24rpx 24rpx; text-align:right;}
.orderBottomBtn button{display:inline-block;}
.orderBottomBtn text{display:inline-block; font-size:26rpx; color:#d81e06;}
.orderBottomBtn label{display:inline-block; font-size:38rpx; color:#d81e06;}
.orderBottomBtn button{margin-left:10rpx;}
.order-btn-gray{background:#fff; line-height:56rpx; font-size:26rpx; border:1rpx #ddd solid; height:56rpx; border-radius:4rpx; padding:0 24rpx;}
.order-btn-org{background:#d81e06; color:#fff; line-height:56rpx; font-size:26rpx; border:0; height:56rpx; border-radius:4rpx; padding:0 24rpx;}
.orderListBottom{border-top:1rpx #ddd solid;}

.orderDetailAddress{background:#fff url(https://cdn.momsurprise.com/static/img/address-bg.png) no-repeat bottom; background-repeat:repeat-x; background-size:40% auto; position:relative; margin-bottom:20rpx; border-radius:0 0  10rpx 10rpx; padding:24rpx 48rpx 28rpx 24rpx; overflow:hidden; display:flex; }
.orderAddressIcon{font-size:40rpx; color:#666;}
.addressUser{font-size:28rpx; display:flex; }
.addressUser text{margin-right:100rpx;}
.orderAddress{width:calc(100% - 60rpx); margin-left:10rpx;}
.orderAddressR{font-size:28rpx; color:#999; position:absolute; right:24rpx; top:50%; margin-top:-28rpx;}
.receivingAddress{font-size:24rpx; padding-top:10rpx;}
.addAddress{width:100%; padding:10rpx 0; display:flex; align-items:center; justify-content:start; font-size:26rpx; color:#333;}
.addAddress label{text-align:center; margin-right:10rpx; font-size:32rpx; color:#fff; text-align:center; line-height:48rpx; width:48rpx; height:48rpx; border-radius:6rpx; background:#3283f9;}
.orderGoodList{background:#fff; margin:20rpx 0; border-radius:10rpx;}
.goodGroup{display:flex; justify-content:space-between; align-items:center; padding:24rpx; border-top:1rpx #ddd solid;}
.goodGroup:first-child{border:0;}
.goodGroupImg{width:180rpx; height:180rpx; border-radius:10rpx; overflow:hidden;}
.goodGroupImg image{width:180rpx; height:180rpx; display:block;}
.goodGroupRh{width:calc(100% - 200rpx); margin-left:20rpx; height:180rpx; position:relative;}
.shopcartNumber{display:flex; align-items:center; font-size:24rpx; color:#999;}
.shopcartNumber input{width:80rpx; height:52rpx; color:#333; text-align:center; font-size:28rpx;}
.shopcartNumber button{background:#ccc; margin:0; color:#fff; width:52rpx; height:52rpx; padding:0px; text-align:center; line-height:52rpx; font-size:28rpx; border-radius:4rpx; border:0rpx;}
.goodGroupRh .h4{font-size:28rpx; height:80rpx;}
.goodGroupRh .p{font-size:24rpx; color:#999;}
.goodGroupRh .p text{padding-right:48rpx;}
.goodsettlementBot{display:flex; align-items:flex-end; justify-content:space-between; position:absolute; bottom:0; left:0; right:0;}
.goodGroupPrice{color:#fe4f4f; font-size:28rpx;}
.blanceMoney{background:#fff; margin:20rpx 0; border-radius:10rpx; padding:10rpx 24rpx;}
.blanceMoneyItem{display:flex; justify-content:space-between; line-height:70rpx; font-size:26rpx;}
.blanceMoneyItem view text{color:#fe4f4f;}
.blanceMoneyItem view{color:#fe4f4f;}
.blanceMoneyItem view.switch{display:flex; align-items:center;}
.blanceMoneyItem view.switch switch{margin-left:20rpx;}
.blanceInfo{background:#fff; padding:20rpx 24rpx; border-radius:10rpx; overflow:hidden;}
.blanceInfo{overflow:hidden; line-height:64rpx;}
.blanceInfo text{float:left; font-size:26rpx; width:150rpx;}
.blanceInfo input{float:left; font-size:26rpx; height:64rpx; width:calc(100% - 150rpx); border:0;}
.blanceFooter{position:fixed;box-shadow:0 0 20rpx rgba(0, 0, 0, 0.05); z-index:99; padding:0rpx 0 0 24rpx; display:flex; align-items:center; justify-content:space-between; bottom:0; background:#fff; left:0; right:0;}
.blanceMoney{background:#fff; padding:20rpx 24rpx; font-size:26rpx; overflow:hidden; margin:20rpx 0; border-radius:10rpx;}
.shopcartBlance button{background:linear-gradient(to right, #d81e06, #d81e06); color:#fff; font-size:28rpx; border:0; line-height:88rpx; height:88rpx; width:240rpx; border-radius:0rpx;}
.shopcartMoney{font-size:26rpx;}
.shopcartMoney text{color:#fe4f4f; font-size:32rpx; font-weight:bold;}
.radio{transform:scale(0.8);}
.dialogDetailBtn{overflow:hidden; padding:24rpx 150rpx;}
.dialogDetailBtn button{color:#fff; border-radius:40rpx; background:linear-gradient(to right, #22d2fd, #d81e06);   font-size:28rpx; border:0; width:100%; height:72rpx; line-height:72rpx; margin:0;}

.orderDetailService{background:#fff; display:flex; justify-content:center; border-top:1rpx #e2e2e2 solid;}
.orderDetailServiceItem{width:49%; position:relative; }
.orderDetailService button{line-height:88rpx; margin:0; width:100%; display:flex; align-items:center; justify-content:center; background:transparent; font-size:26rpx; color:#0098e9;}
.orderDetailService button label{font-size:38rpx; padding-right:10rpx;}
.orderDetailServiceItem:nth-of-type(2)::before{content:""; position:absolute; left:0; top:24rpx; height:40rpx; width:1rpx; background:#e2e2e2;}
