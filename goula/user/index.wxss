@import "/icon.wxss"

.user-container{padding-bottom:100rpx; overflow:hidden;}
.userTop{margin-bottom:20rpx; position:relative; background-color: #d81e06; background-size:100% auto;}
.notice{position:absolute; right:24rpx; top:20rpx;}
.notice text{background:#ffde00; color:#872101; text-align:center; border-radius:30rpx; font-size:20rpx; min-width:28rpx; line-height:28rpx; position:absolute; right:-10rpx; top:-4rpx;}
.notice-icon{background-image: url('https://jingxi-pro.oss-cn-shenzhen.aliyuncs.com/static/v2/img/notice-ico.png'); background-repeat:no-repeat; display:block; background-size:42rpx 42rpx; width:42rpx;  height:42rpx;}
.userInfo{position:relative; z-index:99; padding:20rpx 24rpx 48rpx 38rpx; display:flex; align-items:center;}
.qrCode{position:absolute; padding:8rpx; border-radius:60rpx 0 0 60rpx; background: linear-gradient(to right, #21d4fd, #b721ff);  right:0rpx; font-size:36rpx; bottom:28rpx; color:#fff; display:flex; align-items:center;}
.qrCode text{background:rgba(255,255,255,0.9); margin-right:10rpx; color:#b721ff; width:52rpx; height:52rpx; text-align:center; line-height:52rpx; border-radius:100%;}
.qrCode label{font-size:24rpx;}
.userInfoPic{width:128rpx; height:128rpx; position:relative;  border:4rpx #f3e4a8 solid;  border-radius:100%;overflow:hidden;}
.userInfoPic open-data{width:100%; display:block; border-radius:100%;}
.userInfoPic icon{position:absolute; z-index:999999; width:60rpx;  top:-21rpx; left:-34rpx;}
.userInfoPic icon image{width:60rpx; display:block; border:0; border-radius:0;}
.userInfoText{padding-left:20rpx; color:#fff; font-size:28rpx;}
.userInfoText .name{font-size:30rpx; display:flex; align-items:center;}
.userInfoPic label{background:linear-gradient(to top, #ffc900, #fffc00); display:none; position:absolute; right:4rpx; bottom:0; height:24rpx; width:48rpx; text-align:center; line-height:24rpx; border-radius:4rpx; font-size:18rpx; color:#fe4f4f;}
.userInfoText .codeId{font-size:24rpx; display:flex; align-items:center; margin-top:10rpx; color:rgba(255,255,255,0.8);}
.userInfoText .codeId button{background:rgba(0,0,0,0.2); border-radius:4rpx; margin-left:10rpx; font-size:20rpx; height:38rpx; line-height:38rpx; color:#fff;}
.userInfoText .eEmpiricalValue{font-size:24rpx; padding-top:20rpx;}
.userInfoText .eEmpiricalValue label{font-size:20rpx;}
.userChartBox{background:#fff; position:relative; z-index:8; margin:0 24rpx; border-radius:10rpx; font-size:28rpx;}
.userChartNum{display:flex; align-items:center; justify-content:space-between; padding:18rpx 30rpx 24rpx 30rpx;}
.userTopChart{display:flex; align-items:center; justify-content:space-between; border-bottom:1rpx #ddd solid;  padding:20rpx 24rpx; position:relative; z-index:8; font-size:28rpx;}
.userTopChart button{margin:0; width:140rpx; border-radius:40rpx; background:linear-gradient(to right, #21d4fd, #b721ff); box-shadow: 0 4rpx 10rpx rgba(111, 123, 255, 0.2); font-size:26rpx; color:#fff; line-height:52rpx; }
.userTopChartMoney{display:flex; align-items:center; color:#666; font-size:28rpx;}
.userTopChartMoney text{color:#000; margin-left:10rpx; font-size:36rpx;}
.userTopChartBtn{display:flex; align-items:center;}
.userTopChartBtn .cashBtn{background:#fff; margin-right:20rpx; color:#fe4f4f;}
.userTopChartItemName{font-size:22rpx; color:#666;}
.userTopChartItemName label{font-size:18rpx;}
.right_title{color:#999}
.userTopChartItemNum{font-size:36rpx; display:flex; align-items:flex-end; justify-content:center;}
.userTopChartItemNum text{font-size:28rpx;  position:relative; bottom:2rpx;}
.userTopChartItem{text-align:center;}
.userNavList{margin:20rpx 24rpx; background:#fff; border-radius:10rpx; overflow:hidden;}
.userNavItem{line-height:85rpx; flex-wrap:wrap; padding:0rpx; display:flex; align-items:center; justify-content:space-between;}
.userNavItem button{background:transparent; line-height:85rpx; flex:1;font-weight: 400; border:0; text-align:left; padding:0; display:flex; align-items:center; justify-content:space-between;}
.userNavItemTitle icon{display:inline-block; margin-left:24rpx; text-align:right; width:38rpx; vertical-align:middle;}
.userNavItemTitle icon image{width:38rpx; height:38rpx; display:block;}
.userNavItemTitle text{margin:0 10rpx; font-size:26rpx;}
.userNavItemTitle{display:flex; align-items:center;}
.userNavItemRh{font-size:24rpx; margin-right:20rpx; color:#666; position:relative;}
.userNavItemRh label{color:#999;}
.userNavItemRh icon{margin:0;}
.myInviterTitle{width:100%; display:flex; justify-content:space-between; align-items:center; position:relative;}
.myInviter{display:flex; align-items:center; line-height:30rpx; padding:0 24rpx 24rpx 24rpx;}
.myInviterPic{width:64rpx; height:64rpx; margin-right:10rpx; border-radius:100%; overflow:hidden;}
.myInviterPic image{width:64rpx; height:64rpx; display:block;}
.myInviterInfo text{display:block; font-size:24rpx;}
.myInviterInfo label{color:#999; font-size:20rpx;}
.inviteCodeBd{display:flex; align-items:center; padding:0 24rpx 0rpx 24rpx; width:100%; transform:translateY(0rpx); height:0; opacity:0; transition: all 0.3s ease-in-out 0s; }
.inviteCodeBd input{background:#f9f2ff; padding:0 20rpx; width:480rpx; border:0; border-radius:6rpx; height:64rpx; font-size:26rpx; }
.inviteCodeBd button{line-height:64rpx; margin:0 0 0 20rpx; justify-content:center; background:#7b69fe; color:#fff; border:0; border-radius:6rpx; font-size:26rpx; text-align:center; width:140rpx;}
.inviteCodeBd.show{height:auto; opacity:1.0; transform:translateY(0rpx); padding:0 24rpx 24rpx 24rpx;}
.orderStatus{margin:0rpx 24rpx; border-radius:10rpx; background:#fff; overflow:hidden;}
.userOrder{width:75%; float:left; display:flex; display: -webkit-flex; -webkit-align-items: center; align-items: center; display:-webkit-box; -webkit-box-align:center;}
.userAllorder{width:25%; text-align:center; float:left; background:url(https://jingxi-pro.oss-cn-shenzhen.aliyuncs.com/static/v2/img/order-line.png) left center no-repeat; background-size:auto 150rpx;}
.userAllorder{display:block; position:relative; padding:32rpx 0 32rpx 1%; color:#333; font-size:24rpx; text-align:center; width:24%; float:right;}
.userAllorder button{padding:0; overflow:inherit; line-height:36rpx; background:transparent; font-size:26rpx;}
.userAllorder b{position:absolute; text-align:center; padding:0 10rpx; top:-12rpx; left:52%; font-size:18rpx; line-height:30rpx; display:block; border-radius:38rpx; min-width:30rpx; height:30rpx; color:#fff; background:#fe4f4f; border:1px #fff solid; font-weight:normal;}
.userAllorder text{padding-top:10rpx; display:block;}
.userAllorder icon{display:block; margin:0 auto; width:48rpx; height:48rpx;}
.userAllorder icon image{width:100%; display:block; margin:0 auto;}
.userOrderItem{display:block; position:relative; padding:32rpx 0; color:#333; font-size:24rpx; -webkit-flex:1; flex:1; -webkit-box-flex:1; -moz-box-flex: 1; -ms-flex: 1; text-align:center;}
.userOrderItem button{padding:0; position:inherit; line-height:36rpx; background:transparent; font-size:26rpx;}
.userOrderItem label{position:absolute; text-align:center; padding:0 6rpx; top:18rpx; left:54%; font-size:20rpx; line-height:32rpx; display:block; border-radius:38rpx; min-width:32rpx; height:32rpx; color:#fff; background:#fe4f4f; border:1rpx #fff solid; font-weight:normal;}
.userOrderItem text{padding-top:10rpx; display:block;}
.userOrderItem icon{display:block; margin:0 auto; width:48rpx; height:48rpx;}
.userOrderItem icon image{width:100%; display:block; margin:0 auto;}
.myTeams{background:#fff; margin:20rpx 24rpx; border-radius:10rpx;}
.myTeamsTitle{border-bottom:1rpx #e6e6e6 solid; line-height:80rpx; padding:0 24rpx; display:flex; justify-content:space-between; align-items:center; font-size:28rpx;}
.myTeamsTitle label{font-size:24rpx; color:#999;}
.myTeamsNum{display:flex; justify-content:space-between; align-items:center; padding:20rpx 24rpx;}
.myTeamsNumAll b{display:block; font-size:34rpx; position:relative; bottom:2rpx;}
.myTeamsNumAll label{font-size:24rpx; color:#666;}
.myTeamsNumAll view{display:flex; align-items:flex-end;}
.myTeamsNumAll text{font-size:42rpx; display:block; }
.myTeamsNumAdd{display:flex; align-items:center;}
.myTeamsNumAddItem{ margin-left:80rpx; text-align:center; position:relative;}
.myTeamsNumAddItem:first-child{margin-left:0;}
.myTeamsNumAddItem:last-child::before{content:""; position:absolute; left:-40rpx; height:64rpx; background:#e6e6e6; width:1rpx; top:50%; margin-top:-32rpx;}
.myTeamsNumAddItem b{display:block; font-size:30rpx; position:relative; bottom:2rpx;}
.myTeamsNumAddItem label{font-size:22rpx; color:#999;}
.myTeamsNumAddItem view{display:flex; justify-content:center; align-items:flex-end;}
.myTeamsNumAddItem text{font-size:38rpx; display:block; }
